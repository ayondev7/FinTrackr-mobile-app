{
  "expo": {
    "name": "FinTrackr",
    "slug": "fintrackr-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ayon.fintrackr",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.webp",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "6eb7beaa-d523-4b49-b881-7a788055c0fb"
      }
    }
  }
}
